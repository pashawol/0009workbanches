mixin man()
	.col-xl-7.col-lg-6.align-self-end.order-lg-first.order-last
		block
		+e.img-wrap
			img.lazy.res-i(src="#" data-src="img/@2x/man.png", alt="")
			+e.label
				+e.label-name Александр Сикерин
				+e.label-prof руководитель 
mixin s-form()
	// start s-form
	+b.s-form.section.block-with-lazy#s-form(style="background-image: url(img/@2x/s-form-bg.jpg);")&attributes(attributes)
		.container
			.row

				.col-xl-6.col-lg-6
					+b.section-title
						h2 #[strong Получите полный каталог и&nbsp;прайс] на&nbsp;металлическую  мебель  Виртуоз-групп 
				.col
					+e.head-text 
						+form-wrap("Получить каталог и прайс")
							+form-tabs() 
								+e.group-title Какую компанию вы&nbsp;представляете?

								+e.row.row.justify-content-between
									+b.LABEL.type-company.col-auto
										+e.INPUT.input(type="radio" name="type-company[]" value="Строительная компания" required)
										+e.icon-wrap
											+icon("branch")
										+e.text Производственная
											| 
											br
											|  компания&nbsp;/ Завод
									
									+b.LABEL.type-company.col-auto
										+e.INPUT.input(type="radio" name="type-company[]" value="Торговая компания" )
										+e.icon-wrap
											+icon("wrench")
										+e.text Автосервис
											| 
											br
											| СТО
									+b.LABEL.type-company.col-auto
										+e.INPUT.input(type="radio" name="type-company[]" value="Частное лицо" )
										+e.icon-wrap
											+icon("users")
										+e.text Я частное
											| 
											br
											|  лицо
							+inputName()
							+button('Получить каталог и прайс')
							+polite

	// end s-form




